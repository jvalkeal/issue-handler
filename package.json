{
  "name": "issue-handler",
  "version": "0.0.1",
  "private": true,
  "description": "Issue handler action",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/issue-handler.ts",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prerelease": "npm run-script build",
    "release": "git add -f dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvalkeal/issue-handler.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "jvalkeal",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.1",
    "@actions/github": "^4.0.0",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.0",
    "jexl": "^2.3.0",
    "semver": "^7.3.2",
    "typed-rest-client": "1.7.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.0.4",
    "@types/semver": "^6.0.0",
    "@zeit/ncc": "^0.20.5",
    "jest": "^26.6.3",
    "jest-circus": "^26.2.2",
    "lodash": "^4.17.20",
    "nock": "^13.0.5",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build && npm run format"
    }
  }
}
